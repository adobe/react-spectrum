    "@react-spectrum/utils": "^3.0.0",
    {{#if (includes scopes "@react-stately")}}
    "@react-stately/{{packageName}}": "3.0.0-alpha.1",
    {{/if}}
    {{#if (includes scopes "@react-types")}}
