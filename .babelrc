{
  "presets": [ "react", "es2015" ],
  "env": {
    "development": {
      "presets": [ "react-hmre" ]
    },
    "test": {
      "presets": []
    }
  },
  "plugins": [
    "syntax-class-properties",
    "syntax-decorators",
    "syntax-trailing-function-commas",
    "syntax-object-rest-spread",
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-object-rest-spread"
  ]
}
