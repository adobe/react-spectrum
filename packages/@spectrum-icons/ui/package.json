  },
  "dependencies": {
    "@adobe/react-spectrum-ui": "^1.0.0",
    "@react-spectrum/icon": "^3.1.0"
  },
  "devDependencies": {
    "@spectrum-icons/build-tools": "^3.0.0-alpha.1",
    "react": "^17.0.0-rc.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
