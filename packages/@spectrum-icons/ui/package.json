{
  "name": "@spectrum-icons/ui",
  "version": "3.1.0",
  "description": "Spectrum UI components in React",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/react-spectrum"
  },
  "scripts": {
    "build-icons": "cross-env BUILD_ENV=production babel --root-mode upward src -d . --extensions '.tsx'",
