{
  "extends": "../../../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "noEmit": false,
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "es2020",
    "types": ["node"],
    "declaration": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"],
  "exclude": [
    "dist",
    "node_modules",
    "s2/dist",
    "react-aria/dist"
  ]
}
