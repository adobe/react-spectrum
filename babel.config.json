{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",
    ["@babel/preset-env",
      {
        "loose": true
      }
    ]
  ],
  "env": {
    "storybook": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "targets": {
              "esmodules": true
            }
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "react-remove-properties",
          {
            "properties": [
              "data-testid"
            ]
          }
        ]
      ]
    }
  },
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "version": "^7.6.2"
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "transform-glob-import",
    "babel-plugin-macros"
  ],
  "sourceType": "unambiguous"
}
